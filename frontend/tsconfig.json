{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@babel/types"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "outDir": "dist"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "server/**/*.ts"
  ]
}
